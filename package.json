{
  "name": "solid-react-ui",
  "version": "1.1.0",
  "description": "A lightweight, production-ready React component library styled with Bootstrap and documented with Storybook.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "style": "dist/index.css",
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "storybook",
    "bootstrap",
    "ui"
  ],
  "author": "Walter Gonzalez",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "storybook": "storybook dev -p 6006",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write .",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook build && gh-pages -d storybook-static"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/react": "^9.0.4",
    "@storybook/react-vite": "^9.0.4",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "homepage": "https://walterglezp.github.io/solid-react-ui"
}
