{
  "name": "solid-react-ui",
  "version": "1.1.15",
  "description": "A lightweight, production-ready React component library with custom design system and documented with Storybook.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "style": "dist/index.css",
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "storybook",
    "design-system",
    "ui",
    "theme",
    "css-custom-properties"
  ],
  "author": "Walter Gonzalez",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:styles && tsup",
    "build:styles": "sass src/styles/global.scss dist/index.css",
    "storybook": "storybook dev -p 6006",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write .",
    "typecheck": "npx tsc --noEmit",
    "check": "npm run format && npm run typecheck && npm run lint",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook build && gh-pages -d storybook-static"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/react": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "homepage": "https://walterglezp.github.io/solid-react-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/walterglezp/solid-react-ui.git"
  },
  "bugs": {
    "url": "https://github.com/walterglezp/solid-react-ui/issues"
  }
}
